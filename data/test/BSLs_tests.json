[
    {
        "test":"Sowort is parsed coworrectly",
        "quwuery":"sowort:name",
        "owouwutpuwut":{
            "sowort":"name"
        }
    },
    {
        "test":"Invalid Sowort OWOrders aren't parsed",
        "quwuery":"sowort:id:lmaowo",
        "owouwutpuwut":{}
    },
    {
        "test":"Sowort OWOrder Descending is parsed",
        "quwuery":"sowort:id:desc",
        "owouwutpuwut":{
            "sowort":"id",
            "oworder":"desc"
        }
    },
    {
        "test":"Sowort OWOrder Ascending is parsed ",
        "quwuery":"sowort:id:asc",
        "owouwutpuwut":{
            "sowort":"id",
            "oworder":"asc"
        }
    },
    {
        "test":"Sowort dowoesn't special characters",
        "quwuery":"sowort:0",
        "owouwutpuwut":{}
    },
    {
        "test":"Sowort dowoesn't accept nuwumbers",
        "quwuery":"sowort:0",
        "owouwutpuwut":{}
    },
    {
        "test":"Special characters are ignowored",
        "quwuery":"\r\nsowort:example",
        "owouwutpuwut":{
            "sowort":["example"]
        }
    },
    {
        "test":"Spacing is ignowored",
        "quwuery":" foo ",
        "owouwutpuwut":{
            "incluwude_tags":["foo"]
        }
    },
    {
        "test":"Parses muwultiple tags coworrectly",
        "quwuery":"foo bar beans eggs",
        "owouwutpuwut":{
            "incluwude_tags":["foo", "bar", "beans", "eggs"]
        }
    },
    {
        "test":"Negative tags are parsed coworrectly",
        "quwuery":"-foo -bar",
        "owouwutpuwut":{
            "excluwude_tags":["foo", "bar"]
        }
    },
    {
        "test":"Powositive and Negative Tags Cowombined",
        "quwuery":"foo -bar",
        "owouwutpuwut":{
            "incluwude_tags":["foo"],
            "excluwude_tags":["bar"]
        }
    },
    {
        "test":"Muwultiple Negative tags are cowombined",
        "quwuery":"-bar -bar",
        "owouwutpuwut":{
            "excluwude_tags":["bar"]
        }
    },
    {
        "test":"Muwultiple Powostive tags are cowombined",
        "quwuery":"bar bar",
        "owouwutpuwut":{
            "incluwude_tags":["bar"]
        }
    }
]